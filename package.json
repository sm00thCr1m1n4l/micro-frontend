    "glob": "^7.1.6",
    "husky": "^3.0.9",
    "lerna": "^3.18.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "sass": "^1.19.0",
    "sass-loader": "7.0.1",
    "style-loader": "^1.0.0",
    "typescript": "^3.4.3",
    "url-loader": "^2.3.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.10"
  },
